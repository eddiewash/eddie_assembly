<h1>Welcome Back</h1>
<h2>Please Come In!</h2>

<%= form_tag sessions_path do %>
  <div>
    <%= label_tag :username %>
    <div>
    <%= text_field_tag :username %>
    </div>
  </div>
  <div>
    <%= label_tag :password %>
    <div>
    <%= password_field_tag :password %>
    </div>
  </div>
  <div class="button"><%= submit_tag "Log in" %></div>

<% end %>